  public static void main(String[]args)
  {
    
      int c=1,sum=0;
      while(c<=1600)
      {
      if(n%c==0)
        {
          
            sum=sum+c;
      }
            c++;
            n++;
      }
                    
      if(sum==n)
      {
        System.out.println("Perfect number");
      }
     
        }
      }

